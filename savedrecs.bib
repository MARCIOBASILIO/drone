@inproceedings{ WOS:000476943700002,
Author = {Valvoda, Josef and Ray, Oliver},
Editor = {Arai, S and Kojima, K and Mineshima, K and Bekki, D and Satoh, K and Ohta, Y},
Title = {From Case Law to Ratio Decidendi},
Booktitle = {NEW FRONTIERS IN ARTIFICIAL INTELLIGENCE (JSAI-ISAI 2017)},
Series = {Lecture Notes in Artificial Intelligence},
Year = {2018},
Volume = {10838},
Pages = {20-34},
Note = {9th JSAI International Symposium on Artificial Intelligence (JASI-ISAI),
   Univ Tsukuba, Tsukuba, JAPAN, NOV 13-15, 2017},
Organization = {Japanese Soc Artificial Intelligence},
Abstract = {This paper is concerned with the task of automatically identifying
   legally binding principles, known as ratio decidendi or just ratio, from
   transcripts of court judgements, also called case law or just cases.
   After briefly reviewing the relevant definitions and previous work in
   the area, we present a novel system for automatically extracting ratio
   from cases using a combination of natural language processing and
   machine learning. Our approach is based on the hypothesis that the ratio
   of a given case can be reliably obtained by identifying statements of
   legal principles in paragraphs that are cited by subsequent cases. Our
   method differs from related recent work by extracting principles from
   the text of the cited paragraphs (in the given case) as opposed to the
   text of the citing paragraphs (in a subsequent case). We conduct our own
   independent small-scale annotation study which reveals that this
   seemingly subtle shift of focus substantially increases reliability of
   finding the ratio. Then, by building on previous work in the automatic
   detection of legal principles and cross citations, we present a fully
   automated system that successfully identifies the ratio (in our study)
   with an accuracy of 72\%.},
