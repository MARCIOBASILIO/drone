@article{ WOS:001339575000011,
Author = {Cho, Sang-Hyun and Kim, Dohyun and Kwon, Hyuk-Chul and Kim, Minho},
Title = {Exploring the potential of large language models for author profiling
   tasks in digital text forensics},
Journal = {FORENSIC SCIENCE INTERNATIONAL-DIGITAL INVESTIGATION},
